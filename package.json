{
  "dependencies": {
    "ajv-formats": "^3.0.1",
    "cypress": "^15.7.1",
    "dotenv": "^17.2.3"
  },
  "scripts": {
    "cy:ui": "cypress open --e2e --config specPattern=\"cypress/e2e/ui/**/*.cy.js\"",
    "cy:api": "cypress open --e2e --config specPattern=\"cypress/e2e/api/**/*.cy.js\"",
    "cy:run:ui": "cypress run --spec \"cypress/e2e/ui/**/*.cy.js\"",
    "cy:run:api": "set CYPRESS_API_RUN=true&& cypress run --spec \"cypress/e2e/api/**/*.cy.js\""
  },
  "devDependencies": {
    "ajv": "^8.17.1"
  }
}
